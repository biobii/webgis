<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <title>Maps</title>
  <style>
      /* Always set the map height explicitly to define the size of the div
       * element that contains the map. */
      #map {
        height: 100%;
      }
      /* Optional: Makes the sample page fill the window. */
      html, body {
        height: 100%;
        margin: 0;
        padding: 0;
      }
    </style>
</head>
<body>

  <div id="map"></div>
  
  <script>
      var map;
      function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
          center: {lat: -7.967073, lng: 112.632468},
          zoom: 13
        });

        var marker = new google.maps.Marker({
          position: {lat: -7.962609, lng: 112.623557},
          map: map,
          title: 'Hello World!'
        });
      }
    </script>
    {{--  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAILr1ls6k3ni4MzaMPVOSFBC63OVgPFSA&callback=initMap" async defer></script>  --}}
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDpUxj2k6VK9MgvvJwwqC8b6hqPXWa8iK8&callback=initMap" async defer></script>
</body>
</html>
